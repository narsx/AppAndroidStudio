<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gpixel">

    <application
        android:name=".Usuario.Aplicacion"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gpixel"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_gpixel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DescubrirActivity.Descubrir"
            android:label="@string/title_activity_descubrir"
            />
        <activity
            android:name=".Perfil.PerfilActivity"
            android:label="@string/title_activity_perfil" />
        <activity android:name=".Login.LoginActivity" />
        <activity android:name=".Login.ActivityRegistrar" />
        <activity android:name=".Login.ResetPasswordActivity" />
        <activity android:name=".Login.InicioActivity" />
        <activity android:name=".Contactenos.ActivityNosotros"/>
        <activity android:name=".Login.SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.JuegoActivity" />
    </application>

</manifest>